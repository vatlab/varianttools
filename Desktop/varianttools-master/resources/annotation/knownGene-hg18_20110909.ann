[txEnd]
index=5
type=INTEGER
comment=Transcription end position

[cdsStart]
index=6
type=INTEGER
adj=IncreaseBy(1)
comment=Coding region start

[cdsEnd]
index=7
type=INTEGER
comment=Coding region end

[exonCount]
index=8
type=INTEGER NULL
comment=Number of exons
