[rsname]
index=1
type=VARCHAR(255)
comment=rsname

[chrom]
index=2
type=VARCHAR(255)
adj=RemoveLeading('chr')
comment=chromosome

[pos]
index=3
type=INT
comment=1-based position

[strand]
index=4
type=VARCHAR(1)
comment=strand

[refallele]
index=4,11
type=VARCHAR(255)
adj=lambda x: x[1] if x[0] == '+' else {'A':'T', 'T':'A', 'G':'C', 'C':'G', '-':'-'}[x[1]]
comment=reference allele

[CEU_refallele_freq]
index=12
type=FLOAT
comment=frequency of reference allele

[CEU_refallele_count]
index=13
type=INT
comment=Count of reference allele

[otherallele]
index=4,14
type=VARCHAR(255)
adj=lambda x: x[1] if x[0] == '+' else {'A':'T', 'T':'A', 'G':'C', 'C':'G', '-':'-'}[x[1]]
comment=Other allele

[CEU_otherallele_freq]
index=15
type=FLOAT
comment=frequency of other allele

[CEU_otherallele_count]
index=16
type=INT
comment=Count of other allele

[CEU_totalcount]
index=17
type=INT
comment=Total allele count
