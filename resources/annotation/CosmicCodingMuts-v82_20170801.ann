source_type=txt
source_pattern=

[chr]
index=1
type=VARCHAR(20)
comment=Chromosome

[pos]
index=2
type=INTEGER NOT NULL
comment=1-based position

[COSMIC_ID]
index=3
type=VARCHAR(48)
comment=cosmic id of mutation

[ref]
index=4
type=VARCHAR(255)
comment=Reference allele, '-' for insertion.

[alt]
index=5
adj=CheckSplit()
type=VARCHAR(255)
comment=Alternative allele, '-' for deletion.

[gene]
index=8
type=VARCHAR(255)
adj=ExtractValue('GENE=', ';')
comment=genename

[strand]
index=8
adj=ExtractValue('STRAND=', ';')
type=VARCHAR(255)
comment=strand

[CDS]
index=8
adj=ExtractValue('CDS=', ';')
type=VARCHAR(255)
comment=CDS annotation

[AA]
index=8
adj=ExtractValue('AA=', ';')
type=VARCHAR(255)
comment=Peptide annotation

[CNT]
index=8
adj=ExtractValue('CNT=', ';')
type=INTEGER
comment=Number of samples with this mutation

[SNP]
index=8
adj=ExtractFlag('SNP', ';')
type=INTEGER
comment=classified as SNP